{
  "name": "server",
  "version": "0.0.0",
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^5.2.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "dotenv-cli": "^11.0.0"
  },
  "module": "index.ts",
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "private": true,
  "scripts": {
    "build": "bun build src/server.ts --outdir=dist --target=bun --sourcemap",
    "check": "tsc -b && biome check",
    "dev": "bun run server",
    "format": "biome format",
    "lint": "biome lint",
    "server": "dotenv -e .env -- bun run --watch src/server.ts",
    "start": "bun dist/server.js",
    "tsc": "tsc -b"
  },
  "type": "module"
}
